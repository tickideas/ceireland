{
  "name": "ceireland",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "prisma generate --schema ${PRISMA_SCHEMA:-prisma/schema.prisma}",
    "db:generate": "prisma generate --schema ${PRISMA_SCHEMA:-prisma/schema.prisma}",
    "db:migrate": "prisma migrate dev --schema ${PRISMA_SCHEMA:-prisma/schema.prisma}",
    "db:migrate:create": "prisma migrate dev --create-only --schema ${PRISMA_SCHEMA:-prisma/schema.prisma}",
    "db:seed": "tsx prisma/seed.ts",
    "db:setup": "prisma generate --schema ${PRISMA_SCHEMA:-prisma/schema.prisma} && prisma migrate deploy --schema ${PRISMA_SCHEMA:-prisma/schema.prisma} && tsx prisma/seed.ts",
    "db:deploy": "prisma migrate deploy --schema ${PRISMA_SCHEMA:-prisma/schema.prisma}",
    "db:status": "prisma migrate status --schema ${PRISMA_SCHEMA:-prisma/schema.prisma}"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@prisma/extension-accelerate": "^3.0.1",
    "cheerio": "^1.2.0",
    "valibot": "^1.1.0",
    "domhandler": "^5.0.3",
    "dotenv": "^17.2.3",
    "hls.js": "^1.6.15",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.563.0",
    "next": "^16.1.6",
    "nodemailer": "^7.0.13",
    "pg": "^8.17.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-dropzone": "^14.3.8",
    "recharts": "^3.7.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^16.1.6",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/hls.js": "^1.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.19.7",
    "@types/nodemailer": "^7.0.9",
    "@types/pg": "^8.11.0",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "prisma": "^7.3.0",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
{
  "name": "zchurch",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "prisma generate --schema ${PRISMA_SCHEMA:-prisma/schema.prisma} && prisma db push --schema ${PRISMA_SCHEMA:-prisma/schema.prisma}",
    "db:generate": "prisma generate --schema ${PRISMA_SCHEMA:-prisma/schema.prisma}",
    "db:push": "prisma db push --schema ${PRISMA_SCHEMA:-prisma/schema.prisma}",
    "db:seed": "tsx prisma/seed.ts",
    "db:setup": "prisma generate --schema ${PRISMA_SCHEMA:-prisma/schema.prisma} && prisma db push --schema ${PRISMA_SCHEMA:-prisma/schema.prisma} && tsx prisma/seed.ts",
    "db:push:prod": "prisma db push --schema prisma/schema.prisma",
    "db:migrate:deploy:prod": "prisma migrate deploy --schema prisma/schema.prisma"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@prisma/extension-accelerate": "^2.0.2",
    "bcryptjs": "^3.0.3",
    "cheerio": "^1.1.2",
    "dotenv": "^17.2.3",
    "hls.js": "^1.6.15",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.562.0",
    "next": "^16.1.1",
    "nodemailer": "^7.0.12",
    "pg": "^8.16.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.69.0",
    "recharts": "^3.6.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^16.1.1",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/hls.js": "^0.13.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22",
    "@types/nodemailer": "^7.0.4",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.3",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^9",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "prisma": "^7.2.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "typescript": "^5"
  }
